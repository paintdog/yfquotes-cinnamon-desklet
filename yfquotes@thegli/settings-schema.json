{
  "head": {
    "type": "header",
    "description": "Yahoo Finance Desklet Settings"
  },
  "height": {
    "default": 300,
    "type": "spinbutton",
    "min": 50,
    "max": 1600,
    "description": "Height :",
    "units": "pixels",
    "step": 50,
    "tooltip": "Give the height in pixels."
  },
  "width": {
    "default": 450,
    "type": "spinbutton",
    "min": 50,
    "max": 1600,
    "description": "Width : ",
    "units": "pixels",
    "step": 50,
    "tooltip": "Give the width in pixels."
  },
  "delayMinutes": {
    "default": 10,
    "type": "spinbutton",
    "min": 1,
    "max": 3600,
    "description": "Update frequency : ",
    "units": "minutes",
    "step": 1,
    "tooltip": "Give the update frequency in minutes."
  },
  "companySymbols": {
    "type": "textview",
    "height": 200,
    "default": "MMM\nAXP\nAAPL\nBA\nCAT\nCVX\nCSCO\nKO\nDWDP\nXOM\nGE\nGS\nHD\nINTC\nIBM\nJNJ\nJPM\nMCD\nMRK\nMSFT\nNKE\nPFE\nPG\nTRV\nUTX\nVZ\nWMT\nDIS",
    "description": "Company Symbol List :",
    "tooltip": "Write the company codes here separated with line-breaks."
  },
  "separator": {
    "type": "separator"
  },
  "display": {
    "type": "header",
    "description": "Details of stock quotes to display"
  },
  "showIcon": {
    "type": "checkbox",
    "default": true,
    "description": "Show up/down icon",
    "tooltip": "Shows whether the stock price is up or down"
  },
  "showStockName": {
    "type": "checkbox",
    "default": true,
    "description": "Show full stock name",
    "tooltip": "Shows the full stock name (eg Microsoft Corporation)"
  },
  "showStockSymbol": {
    "type": "checkbox",
    "default": true,
    "description": "Show stock ticker",
    "tooltip": "Shows the stock ticker (eg MSFT)"
  },
  "showStockPrice": {
    "type": "checkbox",
    "default": true,
    "description": "Show stock price",
    "tooltip": "Shows the current stock price"
  },
  "showCurrencyCode": {
    "type": "checkbox",
    "default": true,
    "description": "Show currency code",
    "tooltip": "Shows the currency of the stock price"
  },
  "showStockPercentChange": {
    "type": "checkbox",
    "default": true,
    "description": "Show percent change",
    "tooltip": "Shows the percent change in the stock price"
  }
}
